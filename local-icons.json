{
    "name": "local-icons",
    "buildsystem": "simple",
    "sources": [
        {
            "type": "file",
            "path": "discover-overlay.svg"
        },
        {
            "type": "file",
            "path": "discover-overlay-tray.svg"
        },
        {
            "type": "file",
            "path": "discover-overlay-default.png"
        }
    ],
    "build-commands": [
        "install -d /app/share/icons",
        "install -m644 discover-overlay.svg /app/share/icons/discover-overlay.svg",
        "install -m644 discover-overlay-tray.svg /app/share/icons/discover-overlay-tray.svg",
        "install -m644 discover-overlay-default.png /app/share/icons/discover-overlay-default.png",
    ]
}
